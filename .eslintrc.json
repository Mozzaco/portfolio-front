{
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  }
}
